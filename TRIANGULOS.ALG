Algoritmo "Creuza_01"

Var
   ano_atual, ano_nasc, idade: Inteiro

Inicio
  Escreval ("Em que ano nós estamos? ")
  Leia (ano_atual)
  Escreva ("Em que ano eu nasci? ")
  Leia (ano_nasc)

  idade <- ano_atual - ano_nasc

  Escreva("minha idade é: " , idade)
  
Fimalgoritmo                                                     '